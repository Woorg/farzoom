extends /blocks/layout-default/layout-default

include /blocks/header/header
include /blocks/main/main
include /blocks/slider/slider
include /blocks/clients/clients
include /blocks/partners/partners

include /blocks/footer/footer
include /blocks/popup/popup


block head
	- var pageTitle = 'FArzoom - Лидер в автоматизации банковских бизнес-процессов';

block content
	+header()
	.page__inner
		+main()
		+slider()
		.page__bottom
			.page__in.container
				+clients()
				+partners()
	+footer()
	+popup()

	+popup-success()
